                    </Button>
                    <Button x:Name="button3TeamB" Grid.Column="2" Content="+3" HorizontalAlignment="Center" Margin="60,0,0,0" Grid.Row="2" VerticalAlignment="Top" Width="25" Foreground="#FFCEF3EF" FontFamily="Segoe UI Black" Click="button3TeamB_Click">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black"/>
                                <GradientStop Color="#FF55B4DC" Offset="1"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Button x:Name="button2TeamB" Grid.Column="2" Content="+2" HorizontalAlignment="Center" Margin="0,0,0,0" Grid.Row="2" VerticalAlignment="Top" Width="25" Foreground="#FFCEF3EF" FontFamily="Segoe UI Black" Click="button2TeamB_Click">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black"/>
                                <GradientStop Color="#FF55B4DC" Offset="1"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>

                    <Button x:Name="buttonMinusScoreTeamB" Grid.Column="2" Content="-1" HorizontalAlignment="Center" Margin="0,30,0,0" Grid.Row="2" VerticalAlignment="Top" Width="30" Foreground="#FFE0C92C" FontFamily="Segoe UI Black">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black"/>
                                <GradientStop Color="#FF55B4DC" Offset="1"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>

                    <Button x:Name="buttonMinusScoreTeamA" Grid.Column="1" Content="-1" HorizontalAlignment="Center" Margin="0,30,0,0" Grid.Row="2" VerticalAlignment="Top" Width="30" Foreground="#FFE0C92C" FontFamily="Segoe UI Black" Click="buttonMinusScoreTeamA_Click">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black"/>
                                <GradientStop Color="#FF55B4DC" Offset="1"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>

                    <Label Grid.Column="1" Grid.Row="4" Content="TIMER" HorizontalAlignment="Center" VerticalAlignment="Top" Width="118" Grid.ColumnSpan="2" Foreground="#FF14D2D6" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Padding="0,0,0,0" FontFamily="Segoe UI Black" FontSize="18"/>

                    <TextBox x:Name="tTimer" Grid.Column="1" HorizontalAlignment="Center" Margin="0,0,0,180" Grid.Row="3" TextWrapping="Wrap" VerticalAlignment="Center" Width="160" Grid.ColumnSpan="2" Height="60" Background="#FFC5ECE9" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Arial Black" FontSize="30" />

                    <Button x:Name="buttonStart" Grid.Column="1" Content="Start" HorizontalAlignment="Center" Margin="0,0,100,80" Grid.Row="3" VerticalAlignment="Center" Grid.ColumnSpan="2" Foreground="#FFD5F1F1" Click="buttonStart_Click" Width="46">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black"/>
                                <GradientStop Color="#FF55B4DC" Offset="1"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Button x:Name="buttonStop" Grid.Column="1" Content="Stop" HorizontalAlignment="Center" Margin="0,0,0,80" Grid.Row="3" VerticalAlignment="Center" Grid.ColumnSpan="2" Foreground="#FFD5F1F1" Click="buttonStop_Click" Width="46">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black"/>
                                <GradientStop Color="#FF55B4DC" Offset="1"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Button x:Name="buttonRestart" Grid.Column="1" Content="Resetart" HorizontalAlignment="Center" Margin="100,0,0,80" Grid.Row="3" VerticalAlignment="Center" Grid.ColumnSpan="2" Foreground="#FFD5F1F1" Click="buttonRestart_Click">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black"/>
                                <GradientStop Color="#FF55B4DC" Offset="1"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>




                </Grid>
                <Button Style="{DynamicResource CloseButton}" Content="" BorderThickness="0" BorderBrush="Transparent" HorizontalAlignment="Center" VerticalAlignment="Top" Height="40" Width="40" Margin="0,498,0,0" Click="Button_Click">
                    <Button.Background>
                        <ImageBrush ImageSource="Assets/power-shutdown.png"/>
                    </Button.Background>
                </Button>




            </Grid>
        </Border>
    </Border>
</Window>
